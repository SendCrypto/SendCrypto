  "bugs": {
    "url": "https://github.com/SendCrypto/SendCrypto/issues"
  },
  "homepage": "https://github.com/SendCrypto/SendCrypto#readme",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
