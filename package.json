{
  "name": "sendcrypto",
  "version": "0.0.1",
  "description": "An easy way to send or request cryptocurrency payments. ",
  "main": "dist/startServer.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run webpack",
